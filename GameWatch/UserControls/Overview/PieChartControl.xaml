<UserControl x:Class="GameWatch.UserControls.Overview.PieChartControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GameWatch.UserControls.Overview"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid Background="#FF3E4858">
        <Ellipse
            x:Name="FillEllipse"
            Fill="Red"/>
        <Ellipse
            Stroke="Black"/>
        <Label x:Name="FillLabel" Content="x%" Style="{StaticResource StandartLabelStyle}"/>
    </Grid>
</UserControl>
