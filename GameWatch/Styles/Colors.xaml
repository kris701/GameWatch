<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <FontFamily x:Key="StandartFont">Lucida Console</FontFamily>
    <SolidColorBrush x:Key="StandartFrontColor" Color="White" />
    <SolidColorBrush x:Key="StandartFrontGoodColor" Color="DarkGreen" />
    <SolidColorBrush x:Key="StandartFrontBadColor" Color="DarkRed" />

    <SolidColorBrush x:Key="StandartButtonFrontColor" Color="White" />
    <SolidColorBrush x:Key="StandartDisabledButtonColor" Color="Gray" />
    <SolidColorBrush x:Key="StandartButtonColor" Color="Transparent" />
    <SolidColorBrush x:Key="StandartButtonColorHover" Color="#FFDED3D3" />

    <SolidColorBrush x:Key="StandartTextboxFrontColor" Color="White" />
    <SolidColorBrush x:Key="StandartTextboxBackgroundColor" Color="#FF3A3E46" />

    <SolidColorBrush x:Key="StandartScrollViewBackground" Color="#FF1F252F" />
</ResourceDictionary>